{
  "name": "react-display",
  "version": "1.2.1",
  "description": "React components for helping with inline styling",
  "main": "lib/index.js",
  "scripts": {
    "prestart": "npm prune && npm update",
    "start": "parallelshell 'npm run test:watch' 'npm outdated'",
    "clean": "rimraf lib",
    "lib": "NODE_ENV=production babel src --out-dir lib",
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R dot 'src/**/*.test.js' && eslint src",
    "test:watch": "onchange src -- npm test",
    "cov": "NODE_ENV=test babel-node node_modules/.bin/isparta cover --report text --report lcovonly --report html node_modules/.bin/_mocha -- -R dot 'src/**/*.test.js'",
    "postcov": "open coverage/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luiscarli/react-display"
  },
  "author": "Luis Carli",
  "license": "MIT",
  "devDependencies": {
    "@luiscarli/shallow-render": "^1.0.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "coveralls": "^2.11.4",
    "eslint": "^1.7.2",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "isparta": "^3.1.0",
    "mocha": "^2.3.3",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "react": "^0.14.0",
    "rimraf": "^2.4.3"
  },
  "dependencies": {
    "lodash": "^3.10.1"
  },
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "^3.3.0"
  }
}
